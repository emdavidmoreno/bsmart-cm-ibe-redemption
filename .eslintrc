{
  // Extend existing configuration
  // from ESlint and eslint-plugin-react defaults.
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    // Disable `no-console` rule
    "no-console": 0,
    // Give a warning if identifiers contain underscores
    "no-underscore-dangle": 1,
    // Default to single quotes and raise an error if something
    // else is used
    "quotes": [2, "single"],
    // semicolon omission
    "semi": [2, "never"],
    // for 2-space indentation
    "indent": ["error", 2]
  },
  "globals": {
    "define": true,
    "Farenet": true,
    "Farenet2": true,
    "$": true,
    "jQuery": true,
    "angular": true,
    "interstitialFramework": true,
    "insuranceComponent": true,
    "popupFramework": true,
    "fillFareRuleTabCallback": true,
    "fillFareRuleTabCallbackError": true,
    "guestTypeControl": true,
    "formManager": true,
    "interstitialContents": true,
    "errorManager": true,
    "ajax": true,
    "isSendItineraryFromPopup": true,
    "SEND_CONFIRMATION_FORM_NAME": true,
    "ACTION_URL": true,
    "processSend": true,
    "INFO_SEND_TARGET_ELEMENT": true,
    "getFormElementById": true,
    "INPUT_ID": true,
    "BUTTON_SENDING_ID": true,
    "BUTTON_SEND_ID": true,
    "M3_ERRORS_TARGET_ELEMENT": true,
    "ERRORS_TARGET_ELEMENT": true,
    "tContentData": true,
    "editPaymentCard": true,
    "addNewPayment": true,
    "selectPaymentFromProfile": true,
    "multiplePaymentView": true,
    "dataLayer": true,
    "getElementValue": true,
    "getCardIssuingCountry": true,
    "getDeliveryRule": true,
    "setElementValue": true,
    "resetPaymentCurrencyValues": true,
    "showHideCardIssuingCountryInstallments": true,
    "multiplePaymentConfig": true,
    "com": true,
    "errorFramework": true
  }
}
